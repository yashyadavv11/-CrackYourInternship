void sortColors(int* nums, int numsSize){

 int i,j,a;

 for (i = 0; i < numsSize; i++) 
        {
 
            for (j = i + 1; j < numsSize; j++)
            {
 
                if (nums[i] > nums[j]) 
                {
 
                    a =  nums[i];
                    nums[i] = nums[j];
                    nums[j] = a;
 
                }
 
            }
 
        }
}
