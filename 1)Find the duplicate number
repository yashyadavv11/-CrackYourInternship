//This was first approach. But time complexity was more
int findDuplicate(int* nums, int numsSize){
int i,j;
for (i = 0; i < numsSize; i++)
    {
        for (j =i+1; j < numsSize; j++)
        {
            if (nums[i] == nums[j])
            {
                return nums[i];
            }
        }
    }
    return nums[i];
}

//Best approach was tortoise and hear method. Increment slow(tortoise) by one and fast(hear) by two.
int findDuplicate(int *nums, int numsSize)
{
    int slow = nums[0];
    int fast = nums[0];
    do
    {
        slow = nums[slow];         //+1
        fast = nums[nums[fast]];   //+2
    } while (slow != fast);         

    fast = nums[0];               //when slow==fast reset fast to 0 index and increment it by one now and leave slow as it is.
    while (slow != fast)
    {
        slow = nums[slow];
        fast = nums[fast];
    }
    return slow;
}
